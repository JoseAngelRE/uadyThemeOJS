msgid "plugins.themes.uady.name"
msgstr "Predvolená téma"

msgid "plugins.themes.uady.description"
msgstr "Táto téma implementuje predvolenú tému."

msgid "plugins.themes.uady.option.typography.label"
msgstr "Typografia"

msgid "plugins.themes.uady.option.typography.description"
msgstr "Vyberte kombináciu typov písma, ktorá vyhovuje tomuto časopisu."

msgid "plugins.themes.uady.option.typography.notoSans"
msgstr ""
"Noto Sans: Bezpätkové digitálne písmo navrhnuté spoločnosťou Google s "
"rozsiahlou podporou jazykov."

msgid "plugins.themes.uady.option.typography.notoSerif"
msgstr "Noto Serif: Pätková verzia digitálneho písma spoločnosti Google."

msgid "plugins.themes.uady.option.typography.notoSerif_notoSans"
msgstr ""
"Noto Serif/Noto Sans: Doplnkové párovanie pätkových nadpisov s bezpätkovým "
"ďalším textom."

msgid "plugins.themes.uady.option.typography.notoSans_notoSerif"
msgstr ""
"Noto Sans/Noto Serif: Komplementárne párovanie bezpätkových nadpisov s "
"pätkovým ďalším textom."

msgid "plugins.themes.uady.option.typography.lato"
msgstr "Lato: Populárny moderný bezpätkový font."

msgid "plugins.themes.uady.option.typography.lora"
msgstr "Lora: Široká sada pätkového písma pre dobrú on-line čitateľnosť."

msgid "plugins.themes.uady.option.typography.lora_openSans"
msgstr ""
"Lora/Open Sans: Komplementárne párovanie pätkových nadpisov s bezpätkovým "
"ďalším textom."

msgid "plugins.themes.uady.option.colour.label"
msgstr "Farba"

msgid "plugins.themes.uady.option.colour.description"
msgstr "Zvoľte farbu pre záhlavie."

msgid "plugins.themes.uady.option.showDescriptionInJournalIndex.option"
msgstr "Ukázať na úvodnej stránke časopisu súhrnné informácie o ňom."

msgid "plugins.themes.uady.option.useHomepageImageAsHeader.label"
msgstr "Obrázok na pozadí záhlavia"

msgid "plugins.themes.uady.option.useHomepageImageAsHeader.description"
msgstr ""
"Po nahratí obrázku domovskej stránky sa zobrazí na pozadí záhlavia miesto "
"obvyklej pozície na úvodnej stránke."

msgid "plugins.themes.uady.option.useHomepageImageAsHeader.option"
msgstr "Zobraziť obrázok úvodnej stránky ako pozadie záhlavia."
