msgid "plugins.themes.uady.name"
msgstr "ڕووکاری بنەڕەتی"

msgid "plugins.themes.uady.description"
msgstr "ئەم ڕوکارە پەیڕەوی ڕوکاری بنەڕەتی دەکات."

msgid "plugins.themes.uady.option.typography.label"
msgstr "چاپکاری"

msgid "plugins.themes.uady.option.typography.description"
msgstr "فۆنتێک هەڵبژێرە کە لەگەڵ ئەم گۆڤارە بگونجێت."

msgid "plugins.themes.uady.option.typography.notoSans"
msgstr ""
"Noto Sans: فۆنتی دیجیتاڵیی ڕەسەن کە لە لایەن گوگڵەوە دیزاین کراوە بە مەبەستی "
"پاڵپشتیی چڕی زمان."

msgid "plugins.themes.uady.option.typography.notoSerif"
msgstr ".Noto Serif: یەکێک لە فۆنتە ڕەسەنەکانی گوگڵ"

msgid "plugins.themes.uady.option.typography.notoSerif_notoSans"
msgstr ""
"Noto Serif/Noto Sans: شێوازی تەواوی فۆنتی serif بۆ سەرە بابەتەکان و sans-"
"serif ناو دەقەکان."

msgid "plugins.themes.uady.option.typography.notoSans_notoSerif"
msgstr ""
"Noto Sans/Noto Serif: فۆنتی تەواوی  sans-serif بۆ سەرە بابەتەکان و serif ناو "
"دەقەکان."

msgid "plugins.themes.uady.option.typography.lato"
msgstr "Lato: فۆنتی sans-serif نوێ و بەربڵاو."

msgid "plugins.themes.uady.option.typography.lora"
msgstr "Lora: فۆنتی serif بۆ خوێندنەوە لە ئۆنلاین گونجاوە."

msgid "plugins.themes.uady.option.typography.lora_openSans"
msgstr ""
"Lora/Open Sans: شێوەی تەواوی فۆنتی serif بۆ سەرە بابەتەکان sans-serif بۆ ناو "
"دەق."

msgid "plugins.themes.uady.option.colour.label"
msgstr "ڕەنگ"

msgid "plugins.themes.uady.option.colour.description"
msgstr "ڕەنگ هەڵبژێرە بۆ سەرپەڕە."

msgid "plugins.themes.uady.option.showDescriptionInJournalIndex.option"
msgstr "پوختەی گۆڤار لە پەڕی سەرەکی پیشان بدە."

msgid "plugins.themes.uady.option.useHomepageImageAsHeader.label"
msgstr "وێنەی پاشبنەمای سەرپەڕە"

msgid "plugins.themes.uady.option.useHomepageImageAsHeader.description"
msgstr ""
"کاتێک وێنەی پەڕەی سەرەکی بار کرا، لە پاشبنەمای سەرپەڕە دەردەکەوێت لە جیاتی "
"جێگای خۆی لە پەڕەی سەرەکی."

msgid "plugins.themes.uady.option.useHomepageImageAsHeader.option"
msgstr "وێنەی پەڕەی سەرەکی لە پاشبنەمای سەرپەڕە پیشان بدە."
